{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Client Test",
			"program": "${workspaceFolder}/src/main/client/Test.js",
			"skipFiles": [
				"<node_internals>/**"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${workspaceFolder}/node_modules/electron-webpack/out/cli.js",
			"args": [
				"dev"
			],
			"skipFiles": [
				"<node_internals>/**"
			],
			"preLaunchTask": "Compile Typescript",
			"outputCapture": "std",
			"protocol": "inspector",
		},
	]
}