{
	"name": "client_ts",
	"version": "0.0.0",
	"author": "r.leogrande<leogranderoberto@gmailcom>",
	"description": "My client side electron app",
	"config": {
		"name": "ClientTS"
	},
	"license": "",
	"main": "./dist/electron/main.js",
	"scripts": {
		"dev": "electron-webpack && electron-webpack dev",
		"update-dist": "electron-webpack",
		"update-dist-pack": "electron-webpack && electron-builder",
		"update-dist-pack:dir": "npm run update-dist -- --env.production=true --env.minify=true && electron-builder --dir -c.compression=store -c.mac.identity=null",
		"postinstall": ""
	},
	"electronWebpack": {
		"staticSourceDirectory": "static",
		"whiteListedModules": [
			"electron",
			"vue-router"
		],
		"main": {
			"webpackConfig": "webpack.main.additions.js",
			"sourceDirectory": "src/main"
		},
		"renderer": {
			"webpackConfig": "webpack.renderer.additions.js",
			"template": "src/template.html"
		}
	},
	"build": {
		"productName": "ClientTS",
		"appId": "ClientTS",
		"directories": {
			"output": "build"
		},
		"asar": true,
		"files": [
			"!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
			"!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
			"!**/node_modules/*.d.ts",
			"!**/node_modules/.bin",
			"!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
			"!.editorconfig",
			"!**/._*",
			"!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
			"!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
			"!**/{appveyor.yml,.travis.yml,circle.yml}",
			"!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
			"dist/**/*",
			{
				"from": "./static",
				"to": "static"
			}
		],
		"electronDownload": {
			"cache": "cache"
		},
		"win": {
			"icon": "build/icons/icon.ico"
		}
	},
	"dependencies": {
		"electron-updater": "4.3.1",
		"follow-redirects": "1.13.0",
		"mime-types": "2.1.27",
		"source-map-support": "0.5.19",
		"vue": "2.6.11",
		"vue-router": "3.3.4",
		"websocket": "1.0.31"
	},
	"devDependencies": {
		"@types/follow-redirects": "1.13.0",
		"@types/mime-types": "2.1.0",
		"@types/node": "12.12.38",
		"@types/source-map-support": "0.5.2",
		"@types/webpack": "4.41.21",
		"@types/websocket": "1.0.1",
		"electron": "8.5.2",
		"electron-builder": "22.7.0",
		"electron-webpack": "2.8.2",
		"electron-webpack-ts": "4.0.1",
		"electron-webpack-vue": "2.4.0",
		"ts-loader": "8.0.1",
		"typescript": "4.0.3",
		"vue-loader": "15.9.3",
		"vue-property-decorator": "9.0.0",
		"vue-template-compiler": "2.6.11",
		"webpack": "4.43.0"
	}
}
